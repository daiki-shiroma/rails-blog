<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>

<%= form_for(@user) do |f| %>

<%= f.label :username, "ユーザーネーム" %>
<%= f.text_field :username %>

<%= f.label :profile, "プロフィール" %>
<%= f.text_area :profile %>

<%= f.label :email, "メールアドレス" %>
<%= f.email_field :email %>

<%= f.submit %>

<% end %>